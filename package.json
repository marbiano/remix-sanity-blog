{
  "private": true,
  "name": "remix-app-template",
  "description": "",
  "license": "",
  "scripts": {
    "dev": "cross-env NODE_ENV=development run-p dev:*",
    "dev:remix": "remix watch",
    "dev:wrangler": "dotenv -- cross-var wrangler pages dev ./public --watch ./build --binding SANITY_PROJECT_ID=%SANITY_PROJECT_ID%",
    "dev:css": "postcss styles/**/*.css --dir app/styles --watch",
    "build": "cross-env NODE_ENV=production npm run build:css && npm run build:remix",
    "build:css": "postcss styles/**/*.css --dir app/styles --env production",
    "build:remix": "remix build",
    "prebuild": "rimraf ./public/build \"./app/styles/**/*.css\"",
    "postinstall": "remix setup cloudflare-pages"
  },
  "dependencies": {
    "@remix-run/cloudflare-pages": "^1.1.1",
    "@remix-run/react": "^1.1.1",
    "@sanity/block-content-to-react": "^3.0.0",
    "@sanity/image-url": "^1.0.1",
    "picosanity": "^3.0.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remix": "^1.1.1"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.2.0",
    "@remix-run/dev": "^1.1.1",
    "@tailwindcss/typography": "^0.5.0",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "autoprefixer": "^10.4.1",
    "cross-env": "^7.0.3",
    "cross-var": "^1.1.0",
    "cssnano": "^5.0.14",
    "dotenv-cli": "^4.1.1",
    "esbuild": "0.13.14",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.5",
    "postcss-cli": "^9.1.0",
    "tailwindcss": "^3.0.10",
    "typescript": "^4.1.2",
    "wrangler": "^0.0.0-9cef492"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false,
  "main": "dist/worker.js"
}
